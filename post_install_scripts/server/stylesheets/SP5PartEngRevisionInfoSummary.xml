<!-- Default style sheet for displaying Commercial Part Revision Info Panel in Active Workspace Client. -->
<rendering>
    <header>
        <property name="object_string"/>
    </header>
    <page titleKey="tc_xrt_Summary" visibleWhen="ActiveWorkspace:SubLocation != com.siemens.splm.client.occmgmt:OccurrenceManagementSubLocation">
        <section titleKey="tc_xrt_Preview">
            <image source="thumbnail"/>
        </section>
        <section titleKey="tc_xrt_properties">
            <property name="item_id" renderingHint="label"/>
            <property name="item_revision_id" renderingHint="label"/>
            <property name="object_name"/>
            <property name="object_desc"/>
            <property name="object_type"/>
            <content visibleWhen="REF(items_tag,Part).fnd0AssemblyIndicator==Configurable Assembly">
                <inject type="dataset" src="Smc1VariantConfigContextOnItemRevision"/>
            </content>
            <content visibleWhen="REF(items_tag,Part).fnd0AssemblyIndicator==Generic Part">
                <inject type="dataset" src="Smc1VariantConfigContextOnItemRevision"/>
            </content>
            <break/>
            <property name="release_status_list" renderingHint="label"/>
            <property name="date_released" renderingHint="label"/>
            <property name="effectivity_text" renderingHint="label"/>
            <inject type="dataset" src="Pma1PartRevSummary"/>
            <break/>
            <inject type="dataset" src="Ads1ProjectListInfoRendering"/>
            <break/>
            <property name="owning_user" renderingHint="objectlink" modifiable="false"/>
            <property name="owning_group" renderingHint="objectlink" modifiable="false"/>
            <property name="last_mod_user"/>
            <property name="last_mod_date"/>
            <property name="checked_out" renderingHint="label"/>
            <property name="checked_out_user"/>
            <break/>
            <property name="fnd0CurrentLocationCode"/>
            <property name="REF(items_tag,CommercialPart).fnd0OriginalLocationCode"/>
            <break/>
        </section>
		    <section titleKey="Sierra Space Properties" title="Sierra Space Properties">	
				<property name="sp5AbsPartPropStr1"/>
				<property name="sp5AbsPartPropStr2"/>
				<property name="sp5AbsPartPropStr3"/>
				<property name="sp5AbsPartPropStr4"/>
				<property name="sp5AbsPartPropStr5"/>
				<property name="sp5AbsPartPropStr8"/>
				<property name="sp5AbsPartPropStr9"/>
				<property name="sp5AbsPartPropStr10"/>
				<property name="sp5AbsPartPropLStr1"/>
				<property name="sp5EngPartPropCLOV1"/>
				<property name="sp5EngPartPropCLOV2"/>
				<property name="sp5EngPartPropCLOV3"/>
				<property name="sp5EngPartPropCLOV4"/>
				<property name="sp5EngPartPropStr1"/>
				<property name="sp5EngPartPropCLOV5"/>
				<property name="sp5EngPartPropStr2"/>
				<property name="sp5EngPartPropCLOV6"/>
				<property name="sp5EngPartPropCLOV7"/>
				<property name="sp5EngPartPropCLOV8"/>
				<property name="sp5AbsPartPropCLOV1"/>
				<property name="sp5AbsPartPropCLOV2"/>
				<property name="sp5AbsPartPropCLOV3"/>
				<property name="sp5AbsPartPropCLOV4"/>
			</section>            
    </page>
    <page titleKey="attachments" visibleWhen="ActiveWorkspace:SubLocation != com.siemens.splm.client.occmgmt:OccurrenceManagementSubLocation">
        <section titleKey="tc_xrt_Files">
            <objectSet source="Awp0ReferencesProvider.WorkspaceObject,IMAN_reference.Dataset,IMAN_specification.Dataset,IMAN_manifestation.Dataset,IMAN_Rendering.Dataset,TC_Attaches.Dataset,IMAN_UG_altrep.Dataset,IMAN_UG_scenario.Dataset,IMAN_Simulation.Dataset" defaultdisplay="listDisplay" sortby="object_string" sortdirection="ascending">
                <listDisplay>
                    <property name="object_string"/>
                    <property name="object_type"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                </listDisplay>
                <thumbnailDisplay/>
                <treeDisplay>
                    <property name="object_string"/>
                    <property name="object_type"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                </treeDisplay>
            </objectSet>
        </section>
        <section titleKey="tc_xrt_Documents">
            <objectSet source="IMAN_reference.DocumentRevision" sortdirection="ascending" sortby="object_string" defaultdisplay="listDisplay">
                <listDisplay>
                    <property name="object_string"/>
                    <property name="object_type"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                </listDisplay>
                <thumbnailDisplay/>
                <treeDisplay>
                    <property name="object_string"/>
                    <property name="object_type"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                </treeDisplay>
            </objectSet>
        </section>
    </page>
    <content visibleWhen="object_type==CommercialPart Revision" >
        <page titleKey="tc_xrt_VendorParts">
            <column>
                <objectSet source="VMRepresents.ManufacturerPart" defaultdisplay="tableDisplay" sortdirection="descending" sortby="item_id">
                    <listDisplay>
                        <property name="item_id"/>
                        <property name="object_name"/>
                        <property name="object_desc"/>
                        <property name="vm0vendor_ref"/>
                        <property name="vendor_name"/>
                        <property name="vm0company_location"/>
                        <property name="VMRepresents.preferred_status" />
                    </listDisplay>
                </objectSet>
            </column>
        </page>
    </content>
</rendering>